<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Subjects</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @searches.each do |search| %>
      <tr>
        <td><%= search.key %></td>
        <td><%= search.linked %></td>
        <% if logged_in? %>
          <td><%= link_to 'Enroll', enrolls_adder_path({:my_param=> search.key })%></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
