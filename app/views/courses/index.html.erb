<p id="notice"><%= notice %></p>

<h1>Welcome to Courses List</h1>
<table>
  <thead>
    <tr>
      <th>name</th>
      <th>Id Code</th>
      <th>Subjects</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @courses.each do |course| %>
      <tr>
        <td><%= course.name %></td>
        <td><%= course.idep %></td>
        <td><%= course.subjects %></td>
      </tr>
    <% end %>
  </tbody>
</table>
