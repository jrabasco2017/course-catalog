<p id="notice"><%= notice %></p>

<h1>Welcome to Instructors list</h1>
<table>
  <thead>
    <tr>
      <th>first</th>
      <th>last</th>
      <th>identification</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @instructors.each do |instructor| %>
      <tr>
        <td><%= instructor.first %></td>
        <td><%= instructor.last %></td>
        <td><%= instructor.ident %></td>
      </tr>
    <% end %>
  </tbody>
</table>
