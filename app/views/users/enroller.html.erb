<p id="notice"><%= notice %></p>

<h1>Courses Enrolled</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% current_user.enrollments.each do |enroll| %>
      <tr>
        <td><%= enroll.course %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
