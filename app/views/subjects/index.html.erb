<p id="notice"><%= notice %></p>

<h1>Welcome to Subjects List</h1>
<table>
  <thead>
    <tr>
      <th>name</th>
      <th>identification</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subjects.each do |subject| %>
      <tr>
        <td><%= subject.name %></td>
        <td><%= subject.ider %></td>
      </tr>
    <% end %>
  </tbody>
</table>
